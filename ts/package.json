{
  "name": "repo",
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@9.15.9",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "fmt": "prettier --write \"**/*.{ts,tsx,md}\"",
    "fmt:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.4.4"
  }
}
